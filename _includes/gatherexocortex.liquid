<script type="application/json" id="exocortex-data">
  [
    {% for doc in collections.exocortex %}
      {
        "title" : "{{ doc.title.title }}{% unless doc.title.category_page == true %} - {{ doc.title.category }}{% endunless %}",
        "url"   : "{{ doc.url }}"
      }{% unless forloop.last %},{% endunless %}
    {% endfor %}
  ]
</script>
